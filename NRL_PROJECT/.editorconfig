# .editorconfig
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = crlf
insert_final_newline = true

[*.sh]
end_of_line = lf

[*.cs]
end_of_line = crlf

[*.csproj]
end_of_line = crlf

[*.sln]
end_of_line = crlf

[*.json]
end_of_line = crlf

[Dockerfile]
end_of_line = lf
